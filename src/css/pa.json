{
    "name": "geo_client",
    "description": "Front end for Geo",
    "version": "0.0.3",
    "repository": {
      "type": "git",
      "url": "https://github.com/green-energy-trading/geo-client.git"
    },
    "scripts": {
      "server:dev": "rm -Rf build/* && webpack-dev-server --config webpack_dev.config.js --progress --colors --port 3000 --inline --content-base build/",
      "watch:dev": "rm -Rf build/* && webpack --config webpack_dev.config.js --progress --profile --colors --watch",
      "build:prod": "rm -Rf build/* && NODE_ENV=production webpack --config webpack_production.config.js --progress --profile --colors",
      "build:deploy": "npm run refresh:dependencies && npm run build:prod",
      "update:dependencies:local": "npm i ./dependencies/shared/Functional ./dependencies/shared/formatting ./dependencies/shared/UI ./dependencies/shared/serialization ./dependencies/shared/Form",
      "update:dependencies": "npm i && npm run update:dependencies:local",
      "refresh:dependencies": "rm -Rf node_modules && npm run update:dependencies"
    },
    "devDependencies": {
      "autoprefixer": "^6.7.7",
      "babel-core": "^6.24.1",
      "babel-loader": "^6.4.1",
      "babel-plugin-transform-decorators-legacy": "^1.3.4",
      "babel-polyfill": "^6.23.0",
      "babel-preset-es2015": "^6.24.1",
      "babel-preset-react": "^6.24.1",
      "babel-preset-stage-0": "^6.24.1",
      "baobab": "^2.4.3",
      "baobab-react": "^2.1.2",
      "bowser": "^1.6.1",
      "classnames": "^2.2.5",
      "core-js": "^2.4.1",
      "css-loader": "^0.28.0",
      "deep-diff": "^0.3.4",
      "geo-form": "file:./dependencies/shared/Form",
      "geo-formatting": "file:./dependencies/shared/formatting",
      "geo-functional": "file:./dependencies/shared/Functional",
      "geo-progressbar": "file:./dependencies/shared/ProgressBar",
      "geo-serialization": "file:./dependencies/shared/serialization",
      "geo-ui": "file:./dependencies/shared/UI",
      "highcharts": "^5.0.10",
      "jquery": "~3.2.1",
      "jquery-deparam": "^0.5.3",
      "jquery-serializejson": "^2.8.1",
      "json-loader": "^0.5.4",
      "lodash": "^4.17.4",
      "markdown": "^0.5.0",
      "material-ui": "^1.0.0-beta.6",
      "moment-timezone": "^0.5.13",
      "node-libs-browser": "^2.0.0",
      "node-sass": "^4.5.2",
      "node-uuid": "^1.4.8",
      "pluralize": "^4.0.0",
      "postcss-loader": "^1.3.3",
      "prop-types": "^15.5.8",
      "react": "^15.5.4",
      "react-addons-css-transition-group": "^15.5.2",
      "react-addons-perf": "^15.4.2",
      "react-addons-pure-render-mixin": "^15.5.2",
      "react-dom": "^15.5.4",
      "react-highcharts": "^11.5.0",
      "react-mixin": "^3.0.5",
      "react-router": "^3.0.0",
      "react-select": "^1.0.0-rc.5",
      "sass-loader": "^6.0.3",
      "shallowequal": "^0.2.2",
      "string": "^3.3.3",
      "style-loader": "^0.16.1",
      "superagent": "^3.5.2",
      "webpack": "^2.4.1",
      "webpack-dev-server": "^2.4.2"
    }
  }
  